<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "com.excelr1.Mobile" table = "Mobiles">
        
      <id name = "mobile_no" />
           
      <property name = "model_name" />
      <property name = "company_name"/>  
      <property name = "os_name"/> 
      <property name = "ram_size"/>
      <property name = "rom_size"/>
      <property name = "processor_name"/> 
      <property name = "price"/>  
      <property name = "imeiNo"/>  
      
      </class>
</hibernate-mapping>